@article{Basner:2005ee,
author = {Basner, Jodi E and Schwartz, Steven D},
title = {{How enzyme dynamics helps catalyze a reaction in atomic detail: a transition path sampling study.}},
journal = {Journal of the American Chemical Society},
year = {2005},
volume = {127},
number = {40},
pages = {13822--13831},
month = oct,
publisher = {American Chemical Society},
affiliation = {Department of Physiology and Biophysics, Albert Einstein College of Medicine, 1300 Morris Park Avenue, Bronx, New York 10461, USA.},
doi = {10.1021/ja043320h},
pmid = {16201803},
