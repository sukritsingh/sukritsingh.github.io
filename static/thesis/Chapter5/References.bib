title = {{MSM/RD: Coupling Markov state models of molecular kinetics with reaction-diffusion simulations.}},
journal = {The Journal of chemical physics},
year = {2018},
volume = {148},
number = {21},
pages = {214107},
month = jun,
publisher = {AIP Publishing LLC},
affiliation = {Department of Mathematics and Computer Science, Freie Universit{\"a}t Berlin, Arnimallee 6, 14195 Berlin, Germany.},
