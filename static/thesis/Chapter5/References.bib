uri = {\url{papers3://publication/doi/10.1126/science.1123539}}
}

@article{Wagner:2016dya,
author = {Wagner, Jeffrey R and Lee, Christopher T and Durrant, Jacob D and Malmstrom, Robert D and Feher, Victoria A and Amaro, Rommie E},
title = {{Emerging Computational Methods for the Rational Discovery of Allosteric Drugs.}},
journal = {Chemical Reviews},
year = {2016},
volume = {116},
number = {11},
pages = {6370--6390},
month = jun,
publisher = {American Chemical Society},
affiliation = {Department of Chemistry {\&} Biochemistry and {\ddag}National Biomedical Computation Resource, University of California, San Diego , La Jolla, California 92093, United States.},
doi = {10.1021/acs.chemrev.5b00631},
