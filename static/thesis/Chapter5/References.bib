uri = {\url{papers3://publication/doi/10.1038/nature22327}}
}

@article{Paul:2018ix,
author = {Paul, Sanjib and Paul, Tanmoy Kumar and Taraphder, Srabani},
title = {{Reaction Coordinate, Free Energy, and Rate of Intramolecular Proton Transfer in Human Carbonic Anhydrase II.}},
journal = {The Journal of Physical Chemistry B},
year = {2018},
pages = {acs.jpcb.7b10713},
month = mar,
publisher = {American Chemical Society},
affiliation = {Department of Chemistry , Indian Institute of Technology Kharagpur , Kharagpur 721302 , India.},
