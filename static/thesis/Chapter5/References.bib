month = aug,
publisher = {National Acad Sciences},
affiliation = {Department of Pharmacology, University of Texas Southwestern Medical Center, 5323 Harry Hines Boulevard, Dallas, TX 75235-9041, USA.},
doi = {10.1073/pnas.96.17.9539},
pmid = {10449728},
