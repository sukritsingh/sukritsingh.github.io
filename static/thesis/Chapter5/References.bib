month = apr,
affiliation = {Beijing Advanced Innovation Center for Structural Biology, Tsinghua-Peking Joint Center for Life Sciences, School of Medicine, Tsinghua University, Beijing 100084, China. Electronic address: liu_xy@mail.tsinghua.edu.cn.},
