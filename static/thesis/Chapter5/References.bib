@article{Paul:2017ju,
author = {Paul, Fabian and Wehmeyer, Christoph and Abualrous, Esam T and Wu, Hao and Crabtree, Michael D and Sch{\"o}neberg, Johannes and Clarke, Jane and Freund, Christian and Weikl, Thomas R and No{\'e}, Frank},
title = {{Protein-peptide association kinetics beyond the seconds timescale from atomistic simulations.}},
