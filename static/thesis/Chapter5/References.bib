pmid = {30019778},
language = {English},
read = {Yes},
rating = {0},
date-added = {2018-08-09T21:02:26GMT},
date-modified = {2020-05-23T04:58:50GMT},
abstract = {Many proteins exhibit a critical property called allostery, which enables intra-molecular transmission of information between distal sites. Microscopically, allosteric response is closely related to correlated atomic fluctuations. Conventional correlation analysis correlates the atomic fluctuations at two sites by taking the dot product between the fluctuations, which accounts only for the parallel and antiparallel components. Here, we present a singular value decomposition (SVD) method that analyzes the correlation coefficient of fluctuation dynamics with an arbitrary angle between the correlated directions. In a model allosteric system, the second PDZ domain (PDZ2) in the human PTP1E protein, approximately one third of the strong correlations have near-perpendicular directions, which are underestimated in the conventional method. The discrimination becomes more prominent for residue pairs with larger separation. The results of the proposed SVD method are more consistent with the experimentally determined PDZ2 dynamics than those of conventional method. In addition, the SVD method improved the prediction accuracy of the allosteric sites in a dataset of 23 known allosteric monomer proteins. The proposed method may inspire extended investigation not only into allostery, but also into protein dynamics and drug design. This article is protected by copyright. All rights reserved.},
url = {http://doi.wiley.com/10.1002/prot.25586},
local-url = {file://localhost/Users/Sukrit/Documents/Library.papers3/Articles/2018/Yu/Proteins%20Structure%20Function%20and%20Bioinformatics%202018%20Yu.pdf},
file = {{Proteins Structure Function and Bioinformatics 2018 Yu.pdf:/Users/Sukrit/Documents/Library.papers3/Articles/2018/Yu/Proteins Structure Function and Bioinformatics 2018 Yu.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1002/prot.25586}}
}

