month = jan,
affiliation = {MRC Laboratory of Molecular Biology, Francis Crick Avenue, Cambridge CB2 0QH, UK; Department of Drug Design and Pharmacology, University of Copenhagen, Universitetsparken 2, 2100 Copenhagen, Denmark. Electronic address: alexander.hauser@sund.ku.dk.},
