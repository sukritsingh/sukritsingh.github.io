@article{DiGiacomo:2017jm,
author = {DiGiacomo, Vincent and Marivin, Arthur and Garcia-Marcos, Mikel},
title = {{When Heterotrimeric G Proteins Are Not Activated by G Protein-Coupled Receptors: Structural Insights and Evolutionary Conservation.}},
journal = {Biochemistry},
