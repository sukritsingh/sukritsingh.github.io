@article{Jiang:2006dq,
author = {Jiang, Ya-Ping and Ballou, Lisa M and Lu, Zhongju and Li Wan and Kelly, Damon J and Cohen, Ira S and Lin, Richard Z},
title = {{Reversible heart failure in G alpha(q) transgenic mice.}},
journal = {Journal of Biological Chemistry},
year = {2006},
volume = {281},
