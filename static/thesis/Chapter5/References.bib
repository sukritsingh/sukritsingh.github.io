pmid = {29371618},
language = {English},
rating = {0},
date-added = {2018-01-31T16:53:40GMT},
date-modified = {2020-05-23T04:58:29GMT},
abstract = {Almost all the natural or human made systems can be understood and controlled using complex networks. This is a difficult problem due to the very large number of elements in such networks, on the order of billions and higher, which makes it impossible to use conventional network analysis methods. Herein, we employ artificial intelligence (specifically swarm computing), to compute centrality metrics in a completely decentralized fashion. More exactly, we show that by overlaying a homogeneous artificial system (inspired by swarm intelligence) over a complex network (which is a heterogeneous system), and playing a game in the fused system, the changes in the homogeneous system will reflect perfectly the complex network properties. Our method, dubbed Game of Thieves (GOT), computes the importance of all network elements (both nodes and edges) in polylogarithmic time with respect to the total number of nodes. Contrary, the state-of-the-art methods need at least a quadratic time. Moreover, the excellent capabilities of our proposed approach, it terms of speed, accuracy, and functionality, open the path for better ways of understanding and controlling complex networks.},
url = {http://www.nature.com/articles/s41598-018-19356-4},
local-url = {file://localhost/Users/Sukrit/Documents/Library.papers3/Articles/2018/Mocanu/Sci%20Rep%202018%20Mocanu.pdf},
file = {{Sci Rep 2018 Mocanu.pdf:/Users/Sukrit/Documents/Library.papers3/Articles/2018/Mocanu/Sci Rep 2018 Mocanu.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1038/s41598-018-19356-4}}
}

