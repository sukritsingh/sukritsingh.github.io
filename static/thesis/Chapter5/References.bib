year = {2011},
volume = {7},
number = {9},
pages = {e1002201},
month = sep,
publisher = {Public Library of Science},
affiliation = {Department of Biological Chemistry and Molecular Modelling, IQAC-CSIC, Barcelona, Spain.},
doi = {10.1371/journal.pcbi.1002201},
pmid = {21980279},
