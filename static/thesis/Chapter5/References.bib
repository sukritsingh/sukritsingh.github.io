title = {{Dynamic allostery can drive cold adaptation in enzymes.}},
journal = {Nature},
year = {2018},
volume = {218},
pages = {1834},
month = jun,
publisher = {Nature Publishing Group},
affiliation = {Department of Biology, Johns Hopkins University, Baltimore, MD, USA.},
doi = {10.1038/s41586-018-0183-2},
