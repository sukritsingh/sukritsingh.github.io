journal = {Journal of Chemical Theory and Computation},
year = {2018},
pages = {acs.jctc.8b00033},
month = apr,
publisher = {American Chemical Society},
affiliation = {Program in Bioinformatics and Computational Biology , University of North Carolina at Chapel Hill , Chapel Hill , North Carolina 27599 , United States.},
