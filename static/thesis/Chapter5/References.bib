@article{Flock:2017bm,
author = {Flock, Tilman and Hauser, Alexander S and Lund, Nadia and Gloriam, David E and Balaji, Santhanam and Babu, M Madan},
title = {{Selectivity determinants of GPCR-G-protein binding.}},
journal = {Nature},
year = {2017},
volume = {88},
