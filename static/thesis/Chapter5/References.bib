@article{Weber:2015eu,
author = {Weber, Jeffrey K and Shukla, Diwakar and Pande, Vijay S},
title = {{Heat dissipation guides activation in signaling proteins.}},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
year = {2015},
volume = {112},
number = {33},
pages = {10377--10382},
month = aug,
publisher = {National Acad Sciences},
affiliation = {Department of Chemistry, Stanford University, Stanford, CA 94305;},
doi = {10.1073/pnas.1501804112},
pmid = {26240354},
pmcid = {PMC4547217},
language = {English},
read = {Yes},
rating = {0},
date-added = {2016-10-27T16:51:19GMT},
date-modified = {2020-05-23T04:58:27GMT},
abstract = {Life is fundamentally a nonequilibrium phenomenon. At the expense of dissipated energy, living things perform irreversible processes that allow them to propagate and reproduce. Within cells, evolution has designed nanoscale machines to do meaningful work with energy harnessed from a continuous flux of heat and particles. As dictated by the Second Law of Thermodynamics and its fluctuation theorem corollaries, irreversibility in nonequilibrium processes can be quantified in terms of how much entropy such dynamics produce. In this work, we seek to address a fundamental question linking biology and nonequilibrium physics: can the evolved dissipative pathways that facilitate biomolecular function be identified by their extent of entropy production in general relaxation processes? We here synthesize massive molecular dynamics simulations, Markov state models (MSMs), and nonequilibrium statistical mechanical theory to probe dissipation in two key classes of signaling proteins: kinases and G-protein-coupled receptors (GPCRs). Applying machinery from large deviation theory, we use MSMs constructed from protein simulations to generate dynamics conforming to positive levels of entropy production. We note the emergence of an array of peaks in the dynamical response (transient analogs of phase transitions) that draw the proteins between distinct levels of dissipation, and we see that the binding of ATP and agonist molecules modifies the observed dissipative landscapes. Overall, we find that dissipation is tightly coupled to activation in these signaling systems: dominant entropy-producing trajectories become localized near important barriers along known biological activation pathways. We go on to classify an array of equilibrium and nonequilibrium molecular switches that harmonize to promote functional dynamics.},
url = {http://www.pnas.org/lookup/doi/10.1073/pnas.1501804112},
local-url = {file://localhost/Users/Sukrit/Documents/Library.papers3/Articles/2015/Weber/Proc.%20Natl.%20Acad.%20Sci.%20U.S.A.%202015%20Weber.pdf},
file = {{Proc. Natl. Acad. Sci. U.S.A. 2015 Weber.pdf:/Users/Sukrit/Documents/Library.papers3/Articles/2015/Weber/Proc. Natl. Acad. Sci. U.S.A. 2015 Weber.pdf:application/pdf;Proc. Natl. Acad. Sci. U.S.A. 2015 Weber.pdf:/Users/Sukrit/Documents/Library.papers3/Articles/2015/Weber/Proc. Natl. Acad. Sci. U.S.A. 2015 Weber.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1073/pnas.1501804112}}
}

@article{Wolle:2018ft,
author = {Wolle, Patrik and M{\"u}ller, Matthias P and Rauh, Daniel},
