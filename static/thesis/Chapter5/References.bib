@article{Lee:2017ds,
author = {Lee, Sangbae and Devamani, Titu and Song, Hyun Deok and Sandhu, Manbir and Larsen, Adrien and Sommese, Ruth and Jain, Abhinandan and Vaidehi, Nagarajan and Sivaramakrishnan, Sivaraj},
title = {{Distinct structural mechanisms determine substrate affinity and kinase activity of Protein Kinase C$\alpha$.}},
journal = {Journal of Biological Chemistry},
year = {2017},
volume = {292},
number = {39},
pages = {16300--16309},
month = aug,
affiliation = {Beckman Research Institute of the City of Hope, United States.},
