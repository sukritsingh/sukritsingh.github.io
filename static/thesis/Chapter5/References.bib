date-added = {2018-02-26T22:03:26GMT},
date-modified = {2020-05-23T04:58:53GMT},
abstract = {Mini-G proteins are the engineered GTPase domains of G$\alpha$ subunits. They couple to GPCRs and recapitulate the increase in agonist affinity observed upon coupling of a native heterotrimeric G protein. Given the small size and stability of mini-G proteins, and their ease of expression and purification, they are ideal for biophysical studies of GPCRs in their fully active state. The first mini-G protein developed was mini-Gs. Here we extend the family of mini-G proteins to include mini-Golf, mini-Gi1, mini-Go1 and the chimeras mini-Gs/q and mini-Gs/i. The mini-G proteins were shown to couple to relevant GPCRs and to form stable complexes with purified receptors that could be purified by size exclusion chromatography. Agonist-bound GPCRs coupled to a mini-G protein showed higher thermal stability compared to the agonist-bound receptor alone. Fusion of GFP at the N-terminus of mini-G proteins allowed receptor coupling to be monitored by fluorescence-detection size exclusion chromatography (FSEC) and, in a separate assay, the affinity of mini-G protein binding to detergent-solubilised receptors was determined. This work provides the foundation for the development of any mini-G protein and, ultimately, for the structure determination of GPCRs in a fully active state.},
url = {http://dx.plos.org/10.1371/journal.pone.0175642},
local-url = {file://localhost/Users/Sukrit/Documents/Library.papers3/Articles/2017/Nehm%C3%A9/PLoS%20ONE%202017%20Nehm%C3%A9.pdf},
file = {{PLoS ONE 2017 Nehmé.pdf:/Users/Sukrit/Documents/Library.papers3/Articles/2017/Nehmé/PLoS ONE 2017 Nehmé.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1371/journal.pone.0175642}}
}

@article{Pan:2017bp,
author = {Pan, Albert C and Xu, Huafeng and Palpant, Timothy and Shaw, David E},
