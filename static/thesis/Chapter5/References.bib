language = {English},
read = {Yes},
rating = {0},
date-added = {2017-09-29T20:47:24GMT},
date-modified = {2018-04-10T18:55:43GMT},
abstract = {Many enhanced sampling methods rely on the identification of appropriate collective variables. For proteins, even small ones, finding appropriate descriptors has proven challenging. Here we suggest that the NMR S(2) order parameter can be used to this effect. We trace the validity of this statement to the suggested relation between S(2) and conformational entropy. Using the S(2) order parameter and a surrogate for the protein enthalpy in conjunction with metadynamics or variationally enhanced sampling, we are able to reversibly fold and unfold a small protein and draw its free energy at a fraction of the time that is needed in unbiased simulations. We also use S(2) in combination with the free energy flooding method to compute the unfolding rate of this peptide. We repeat this calculation at different temperatures to obtain the unfolding activation energy.},
url = {http://pubs.acs.org/doi/abs/10.1021/acs.jpclett.7b01770},
local-url = {file://localhost/Users/Sukrit/Documents/Library.papers3/Articles/2017/Palazzesi/J.%20Phys.%20Chem.%20Lett.%202017%20Palazzesi.pdf},
file = {{J. Phys. Chem. Lett. 2017 Palazzesi.pdf:/Users/Sukrit/Documents/Library.papers3/Articles/2017/Palazzesi/J. Phys. Chem. Lett. 2017 Palazzesi.pdf:application/pdf;J. Phys. Chem. Lett. 2017 Palazzesi.pdf:/Users/Sukrit/Documents/Library.papers3/Articles/2017/Palazzesi/J. Phys. Chem. Lett. 2017 Palazzesi.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1021/acs.jpclett.7b01770}}
}

