month = aug,
affiliation = {Institut des Biomol{\'e}cules Max Mousseron (IBMM), CNRS UMR5247, Universit{\'e} Montpellier 1-Universit{\'e} Montpellier 2, Facult{\'e} de Pharmacie, 15 Avenue Charles Flahault, BP 14491, 34093 Montpellier Cedex 05, France.},
