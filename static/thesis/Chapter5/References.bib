title = {{Excitation lines and the breakdown of Stokes-Einstein relations in supercooled liquids.}},
journal = {Physical review. E, Statistical, nonlinear, and soft matter physics},
year = {2004},
volume = {69},
number = {6 Pt 1},
pages = {061205},
month = jun,
publisher = {American Physical Society},
