@article{Maeda:2018bu,
author = {Maeda, Shoji and Koehl, Antoine and Matile, Hugues and Hu, Hongli and Hilger, Daniel and Schertler, Gebhard F X and Manglik, Aashish and Skiniotis, Georgios and Dawson, Roger J P and Kobilka, Brian K},
